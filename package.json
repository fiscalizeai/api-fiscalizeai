  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "scraping": "node build/tasks/scraping.js",
    "deleteTmpFiles": "node build/tasks/delete-tmp-files.js",
    "processTmpFiles": "node build/tasks/process-tmp-files.js",
    "scraping:dev": "tsx src/tasks/scraping.ts",
    "deleteFiles:dev": "tsx src/tasks/delete-tmp-files.ts",
    "processFiles:dev": "tsx src/tasks/process-tmp-files.ts"
  },
